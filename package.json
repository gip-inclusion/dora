{
  "name": "dora-front",
  "version": "0.0.20",
  "engines": {
    "npm": ">=8",
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/dora-front"
  },
  "scripts": {
    "dev": "vite dev --port 3000 --strictPort",
    "build": "vite build",
    "start": "node build",
    "preview": "vite preview --port 3000 --strictPort",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --output human-verbose --threshold error",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write . && eslint --fix . && svgo --pretty -f src/lib/assets",
    "svg-optimize": "svgo --pretty -r -f ./src/lib/assets",
    "prepare": "husky install",
    "security-scan": "ggshield secret scan path -r . -y"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "1.3.1",
    "@sveltejs/kit": "1.27.6",
    "@tailwindcss/typography": "0.5.10",
    "@tiptap/core": "2.1.8",
    "@tiptap/extension-link": "2.1.8",
    "@tiptap/extension-placeholder": "2.1.8",
    "@tiptap/starter-kit": "2.1.8",
    "@types/iframe-resizer": "3.5.9",
    "@types/showdown": "2.0.1",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "autoprefixer": "10.4.15",
    "dayjs": "1.11.9",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.33.1",
    "hexoid": "1.0.0",
    "husky": "8.0.3",
    "iframe-resizer": "4.3.6",
    "insane": "2.6.2",
    "lint-staged": "14.0.1",
    "maplibre-gl": "3.3.1",
    "opening_hours": "3.8.0",
    "postcss": "8.4.31",
    "postcss-load-config": "4.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "remixicon": "3.5.0",
    "showdown": "2.1.0",
    "svelte": "4.2.0",
    "svelte-check": "3.5.1",
    "svelte-portal": "2.2.0",
    "svgo": "3.0.2",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.4",
    "wicg-inert": "3.1.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,postcss,html,md}": [
      "prettier --write"
    ],
    "*": [
      "ggshield secret scan pre-commit"
    ]
  },
  "dependencies": {
    "@sentry/sveltekit": "7.64.0"
  }
}
